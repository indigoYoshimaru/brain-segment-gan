    "discriminator": {
        "opt": "Adam",
        "lr": 0.001,
        "decay": 0.0001, 
        "betas": [0.5, 0.999],
        "grad_clip": -1, 
        "epochs":1, 
        "gan_loss": "mse_loss"
    }
}